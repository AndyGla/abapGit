managed; // implementation in class zbp_sla_i_patient_m unique;

define behavior for ZSLA_I_PATIENT_M alias Patient
persistent table ZSLA_PATIENT
lock master
//authorization master ( instance )
//etag master <field_name>
{
field ( readonly, numbering : managed  ) Patnr;


  create;
  update;
  delete;

  mapping for zsla_patient {
        patnr = patnr;
        Gebdat = gebdat;
        Nachname = nachname;
        Vorname = vorname;


   }


}