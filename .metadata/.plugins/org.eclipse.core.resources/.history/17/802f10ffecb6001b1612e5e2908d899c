  gebdat     : char10;
  geschlecht : /su01/sla_ges;
  land       : char3;
  plz        : char10;
  ort        : char40;
  strasse    : char45;
  hausnr     : char9;
  zusatz     : char40;
  postfach   : char8;
  telnr      : char20;
  mobilnr    : char20;
  fax        : char20;
  email      : char40;
  ds1flag    : char1;
  ds1date    : char10;
  eruname    : char10;
  erdat      : char10;
  erzeit     : char6;
